#!/bin/bash

sudo apt update
sudo apt install expect tightvncserver lubuntu-desktop


/usr/bin/expect <<EOF
spawn /usr/bin/vncserver
expect "Password:"
send "syahrul\n"
expect "Verify:"
send "syahrul\n"
expect "Would you like to enter a view-only password (y/n)?"
send "n\n"
interact
EOF

vncserver
vncserver -kill :1
cd ~
sed -i '$ d' .vnc/xstartup > .vnc/xstartup
echo "openbox &" >> .vnc/xstartup
echo "lxsession -s Lubuntu &" >> .vnc/xstartup
vncserver -geometry 1366x768
PPK="-----BEGIN RSA PRIVATE KEY-----\nMIIEpQIBAAKCAQEA115KE0l2mcWyioS5niTejQlD0Qh1rs95lNPS/GvkTCHjrhLl\npkYOcum+ueF5k1Wy/S7zXgLVbhzNIDl7vqos3TGtqFbYNIW0HjXBEeTMVt1HvIK2\nDEFs5amPUhzhm4CIr2uRC93REGp1dhZTUjmdErjJk0YDnU/nbV0MwEq97AhU0aHz\nRRJ5Kq9jcszAm5b7e0eWMG39kOOHJ/oo6qPkk2f52yL331SlJNqZ+jGUQ3XehRDA\nobhj4sAJhG9y1dwHjANscVPcc7zkOiy9nVzO8WZ0KubisXw2ZXuoG2hNg33WkJqD\nOGQb3Geg6EFDSKvofNk7sohS0WzzUG3n5lJCDwIDAQABAoIBAQCFEYg5ZIG4P1Oy\njJFXqjYr9FaHmG+GJEFd+Id7WXqEo8fWrWxhkRWqYMxsc2k35JKkJtdN9fhDgd4o\nE/xmcSUpQmGnComp4hbJN7NkR/cNkMmbKzQqXF5ZhKu+M4QeHYvWv2Kub06WkwCi\nhubSfjCT+cLqkS74iv9WLlELmuLAK760CG1zNBLE3EJMPzS+8ymrczAy8n47ciQq\nDX1VXCzo8KEW/bNB8j3gox8/JVMrUD8cL9uftoL7ykWoUwCstKpafdWNC4aotsJq\nVK16e+frr47TOhk19qNMOjlxldcn27n8eTOaR6kIkFG2a6qrupddelkxU1LpQfjo\nvHQgeFJZAoGBAPyiSnLPZPmt5u8KFERMMfV+WLq98hmeQzHJOS+JctkxxhbP8Uv1\nYhQPdnmmy7m+wp7mVcW6IjO6vewG3/D2h9E/sjablpaZ3et0hsIyMxjAGD8+dBUB\nX3+ebEtIxmJEXGrVNB5qKc91KbL7qemIwhDnotCAqNnmkswyVjvfkjbrAoGBANo8\n46JMO7HEcZHCYJCAetZD3IGlw6hUt6OeBwH46XbTDA2ranRz1dPiKC/3BBl+sQu2\niIF8R/km+4MZ+8+V0enlEFbTBTazq5qivlrcrgYhrjxkMGs6O0rPWE8PkbkwtevQ\n7Ou8FOn8bZo6UZxzkPwUbWKN+d8xPJLqzJREuqBtAoGBAJDAsKWuHa5OLUcD4G7t\ntaZzOYXudfylyNyK7qBFQjAeq+DATwL127eg0IKNxqdsHPmNAtJjf1tigZXurnvT\n1ihYNdXCOnuaOCTERL6aotponrwGHd83RS/LZ0OPtFPz1XxFIzLWI5TCDh2Gw2mT\nOZcUEh8jyRvpWG113iE6Tf33AoGBAIEuM6xZJainWHZoMZK3in6UMs1HWtakNwOR\nwiInvcJJUMQyqFQAUR0XrKQfunyA4rnFNW44K75OlehXlBefXl7t6UsLo/VqSvTA\nyp6sLBKTEzjKMWL3WCuDKom59I9DRxwsUmGDtnl+oz38sVxDiBm+2bTRx4uW8H0i\noTBOso3NAoGAEgK7pg8pW7iceNLb6xh3NkEQTT+lXKi0hjxJmGSpPg9kCFDPRklm\nIQgh4eOMdh1DcFw7fvJjvinbAkTI7TJs9Ni0ENXpYhLy0WuiV0Ahue7i5neLUm3U\nYNphZeLFqmGQmjrcr43NdM5C8p6/a/nyXk9sJjydv+4UA176s8KqETY=\n-----END RSA PRIVATE KEY-----"
